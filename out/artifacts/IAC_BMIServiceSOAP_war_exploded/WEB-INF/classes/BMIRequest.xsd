<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:tns="http://schemas.hu.fnt.nl/berichten/request"
  targetNamespace="http://schemas.hu.fnt.nl/berichten/request">
  <xs:element name="bmirequest" type="tns:request" />
  <xs:complexType name="request">
    <xs:all minOccurs="1">
      <!-- Vul hier je leeftijd en gewicht in -->
      <!-- Het ingevoerde getal moet een geheel positief getal zijn -->
      <xs:element name="leeftijd" type="xs:positiveInteger" />
      <xs:element name="gewicht" type="xs:positiveInteger" />
	</xs:all>
  </xs:complexType>
</xs:schema>